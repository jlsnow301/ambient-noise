{
  "expo": {
    "name": "ambient-noise",
    "slug": "ambient-noise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleMapsApiKey": "./constants/api-keys.js"
      },
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "./constants/api-keys.js"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
